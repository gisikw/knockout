# List all open tickets
exec ko ls
stdout 'list-0001'
stdout 'list-0002'

# List with status filter
exec ko close list-0002
exec ko ls --status=open
stdout 'list-0001'
! stdout 'list-0002'

# List shows deps
exec ko dep list-0001 list-0003
exec ko ls
stdout '<- \[list-0003\]'

