# ko triage lists tickets with a triage value set
exec ko triage
stdout 'ko-triaged'
stdout 'triage: unblock this ticket'

# ko triage excludes tickets without a triage value
exec ko triage
! stdout 'ko-plain'

# ko triage --json outputs JSON with triage field
exec ko triage --json
stdout '"triage":\s*"unblock this ticket"'
! stdout '"id":\s*"ko-plain"'

