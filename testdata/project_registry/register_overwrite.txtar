# Register overwrites existing entry for same tag
env HOME=$WORK/home
mkdir -p $WORK/home/.config/knockout
exec ko register '#myproj'
stdout 'registered myproj'

# Re-register from different directory overwrites path
mkdir $WORK/other/.ko/tickets
cd $WORK/other
exec ko register '#myproj'
stdout 'registered myproj'

# Projects shows only one entry (not duplicated)
exec ko projects
stdout 'myproj'

-- .ko/tickets/.keep --
