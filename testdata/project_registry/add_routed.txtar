# Add with recognized tag routes to target project
env HOME=$WORK/home
mkdir $WORK/home

# Register target project
mkdir $WORK/target/.tickets
cd $WORK/target
exec ko register '#target'
cd $WORK

# Add a routed ticket
exec ko add 'Fix the thing #target'
stdout '#target'

# Verify ticket exists in target project with status routed
env TICKETS_DIR=$WORK/target/.tickets
exec ko query
stdout 'Fix the thing'
stdout '"status":"routed"'

# Verify audit ticket in local project is closed
env TICKETS_DIR=$WORK/.tickets
exec ko query
stdout 'Fix the thing'
stdout '"status":"closed"'

