# Add with --project and --tags routes to target with tags
env HOME=$WORK/home
mkdir $WORK/home

# Register target project
mkdir $WORK/target/.ko/tickets
cd $WORK/target
exec ko project set '#target'
cd $WORK

# Add with --project and --tags
exec ko add 'Build splash page' --project=target --tags=marketing-site,urgent

# Verify ticket in target project with tags preserved
env TICKETS_DIR=$WORK/target/.ko/tickets
exec ko ls --json
stdout 'Build splash page'
stdout 'marketing-site'
stdout 'urgent'

