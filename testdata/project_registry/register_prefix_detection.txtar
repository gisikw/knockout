# project set detects ticket prefix from existing tickets
env HOME=$WORK/home
mkdir $WORK/home

# Create a project with existing tickets (prefix "fn")
mkdir $WORK/fort-nix/.ko/tickets
cp $WORK/fn_ticket.txt $WORK/fort-nix/.ko/tickets/fn-a001.md

# Register it
cd $WORK/fort-nix
exec ko project set '#fort-nix'
stdout 'project fort-nix registered'

# Verify prefix was stored in registry
exec cat $WORK/home/.config/knockout/projects.yml
stdout 'prefix: fn'

