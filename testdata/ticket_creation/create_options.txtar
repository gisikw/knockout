# Create with description
exec ko add -d 'This is the description' 'Test ticket'
exec ko ls --json
stdout 'Test ticket'

# Create with type
exec ko add -t bug 'Bug ticket'
exec ko ls --json
stdout '"type":\s*"bug"'

# Create with priority
exec ko add -p 0 'High priority'
exec ko ls --json
stdout '"priority":\s*0'

# Create with assignee
exec ko add -a 'John Doe' 'Assigned ticket'
exec ko ls --json
stdout '"assignee":\s*"John Doe"'

