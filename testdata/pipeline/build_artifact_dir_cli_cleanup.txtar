# Artifact directory: persists when ticket is closed via CLI
mkdir -p .ko/tickets/ko-a001.artifacts
cp plan.md .ko/tickets/ko-a001.artifacts/plan.md
exists .ko/tickets/ko-a001.artifacts/plan.md
exec ko close ko-a001
stdout 'ko-a001 -> closed'
# Artifact dir should persist
exists .ko/tickets/ko-a001.artifacts
exec cat .ko/tickets/ko-a001.artifacts/plan.md
stdout 'some plan content'

